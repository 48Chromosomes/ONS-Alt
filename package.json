{
  "name": "svelteapp",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "build-node": "export SKADAPTER=node && svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint:prettier": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "format:prettier": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "format:eslint": "eslint --ignore-path .gitignore . --fix"
  },
  "type": "module",
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-node": "^1.0.0-next.72",
    "@sveltejs/kit": "next",
    "@types/d3-dsv": "^3.0.0",
    "@types/mapbox-gl": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.2",
    "dotenv": "^16.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "sass": "^1.49.7",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.0.23",
    "tslib": "^2.3.1",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@ons/design-system": "^45.2.2",
    "@sveltejs/adapter-node": "^1.0.0-next.72",
    "d3-dsv": "^2.0.0",
    "mapbox-gl": "^1.13.2",
    "normalize.css": "^8.0.1",
    "rxjs": "^7.5.4"
  }
}
